{
"name": "Bee Focused",
"description": "An productivity extension that blocks certain websites and has a cute bee theme",
"version": "0.1.0",
"manifest_version": 3,
"content_scripts":[
    {
        "matches":["<all_urls>"],
        "js":["scripts/script.js"]
    }
],
"permissions":[
    "activeTab"
],
"action":{
    "default_popup":"popup.html",
    "default_title": "Bee Focused"
}
} 